{
    "owner_email" : "lachie.murray@gmail.com",
    "publication_api_version":"1.0",
    "name": "parkrun",
    "description": "Provides a fresh barcode and up-to-date news and weather reports for your local parkrun.",
    "delivered_on": "every Saturday",
    "send_timezone_info": false,
    "send_delivery_count": false,
    "external_configuration": false,
    "config" : {
      "fields": [
          {
              "type": "text",
              "name": "barcode",
              "label": "Enter your barcode number"
          },
          {
              "type": "select",
              "name": "event",
              "label": "Select your local parkrun",
              "options": [
                  [
                      "Mile End",
                      "mileend"
                  ],
                  [
                      "York",
                      "york"
                  ],
                  [
                      "Bushy Park",
                      "bushy"
                  ]
              ]
          }
      ]
    }
}